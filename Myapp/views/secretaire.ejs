            <script>
                //basic URL to handle coordonnateur table
                const api_url = 'http://localhost:3000/api/v1/coordonnateur';
                async function getDataFromDB(url) {
                    const response = await fetch(url);
                    // Storing data in form of JSON
                    var data = await response.json();
                    if (response) {
                        console.log(data);
                    }
                    return data
                }
                //execute retrieveCoordonnateurList() with result of getDtaFromDB
                getDataFromDB(api_url).then((data) => {
                    retrieveCoordonnateurList(data)
                });
                // Function to define innerHTML for HTML table
                function retrieveCoordonnateurList(data) {
                    const table_to_fill = document.getElementById("coordonnateur_table")
                    let table_template =
                        `<thead>
                            <tr class="headings">
                                <th class="column-title">id</th>
                                <th class="column-title">Nom </th>
                                <th class="column-title">Mail </th>
                                <th class="column-title"> </th>
                                <th class="column-title"> </th>
                                <th class="bulk-actions" colspan="7"><a class="antoo" style="color:#fff; font-weight:500;">Bulk Actions ( <span class="action-cnt"> </span> ) <i class="fa fa-chevron-down"></i></a></th>
                            </tr>
                        </thead>`;
                    // Loop to access all rows 
                    for (let record of data) {
                        table_template += `<tr class="even pointer">
                    <td>${record.idCoordonnateur} </td>
                    <td>${record.nom}</td>
                    <td>${record.prenom}</td> 
                    <td>${record.mail}</td>          
                    <td class="last"><button class="btn btn-primary">Supprimer</button></td>
                    <td class="last"><button class="btn btn-primary edit_coordo_btn" data-idCoordonnateur=${record.idCoordonnateur} data-toggle="modal" data-target="#coordo_editing_modal">Modifier</button></td>
                            
                </tr>&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;`;
                    }
                    // Setting innerHTML as tab variable
                    table_to_fill.innerHTML = table_template;
                }
                //This code must be executable after the loading of the whole the page
                window.onload = function () {
                    const edit_coordo_btn = document.getElementsByClassName('edit_coordo_btn'),
                        coordo_id_input = document.getElementById('coordonnateur_id')
                    coordo_name_input = document.getElementById('coordonnateur_name'),
                        coordo_firstname_input = document.getElementById('coordonnateur_firstname'),
                        coordo_email_input = document.getElementById('coordonnateur_email');
                    Array.from(edit_coordo_btn).forEach(element => element.addEventListener('click', (e) => {
                        const idCoordonnateur = e.target.getAttribute('data-idCoordonnateur')
                        let new_url = api_url + "/" + idCoordonnateur;
                        getDataFromDB(new_url).then((data) => {
                            //We fill inputs by existing records, the data rray contains only one item
                            coordo_id_input.value = data[0].idCoordonnateur
                            coordo_name_input.value = data[0].nom
                            coordo_firstname_input.value = data[0].prenom
                            coordo_email_input.value = data[0].mail
                        });
                    }))
                    document.getElementById('coordo_form_submit').addEventListener('click', () => {
                        const put_url = api_url + "/" + coordo_id_input.value; //We get id from the coordo_id_input
                        const requestOptions = {
                            method: 'PUT',
                            headers: { 'Content-Type': 'application/json' },
                            body: JSON.stringify({
                                nom: coordo_name_input.value,
                                prenom: coordo_firstname_input.value,
                                mail: coordo_email_input.value
                            })
                        };
                        fetch(put_url, requestOptions)
                            .then(response => response.json())
                            .then(data => console.log(data));
                        //We reload the document page
                        location.reload()
                    })
                }
            </script>
